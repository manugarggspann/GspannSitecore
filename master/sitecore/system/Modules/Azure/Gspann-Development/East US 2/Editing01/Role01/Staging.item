    <WadCfg>
      <DiagnosticMonitorConfiguration overallQuotaInMB="4096">
        <DiagnosticInfrastructureLogs />
        <Directories>
          <IISLogs containerName="wad-iis-logfiles" />
          <FailedRequestLogs containerName="wad-iis-failedreqlogfiles" />
